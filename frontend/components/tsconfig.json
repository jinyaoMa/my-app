{
  "compilerOptions": {
    "target": "ES2022",
    "module": "ESNext",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitAny": false,
    "skipLibCheck": true,
    "lib": ["DOM", "ESNext"],
    "allowImportingTsExtensions": true,
    "noEmit": true
  },
  "exclude": ["node_modules"]
}
