{
  "name": "@jinyaoma/my-app",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test:unit": "vitest",
    "test:e2e": "playwright test",
    "build-only": "vite build",
    "build-dev": "vite build --minify false",
    "type-check": "vue-tsc --build",
    "lint": "eslint --config ./eslint.config.ts . --fix",
    "format": "prettier --write --config ../../.prettierrc.json src/"
  },
  "dependencies": {
    "@jinyaoma/my-components": "workspace:^",
    "@jinyaoma/my-design": "workspace:^",
    "@jinyaoma/my-sdk": "workspace:^",
    "@wailsio/runtime": "3.0.0-alpha.66",
    "pinia": "^3.0.3",
    "vue": "^3.5.20",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@playwright/test": "^1.55.0",
    "@tsconfig/node22": "^22.0.2",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.18.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitejs/plugin-vue-jsx": "^5.1.1",
    "@vitest/eslint-plugin": "^1.3.4",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.34.0",
    "eslint-plugin-playwright": "^2.2.2",
    "eslint-plugin-vue": "~10.2.0",
    "jiti": "^2.5.1",
    "jsdom": "^26.1.0",
    "npm-run-all2": "^8.0.4",
    "prettier": "3.5.3",
    "typescript": "~5.8.3",
    "vite": "^7.1.3",
    "vite-plugin-vue-devtools": "^7.7.7",
    "vitest": "^3.2.4",
    "vue-tsc": "^2.2.12"
